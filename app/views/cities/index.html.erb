<p id="notice"><%= notice %></p>

<h1>Listing Cities</h1>

<table class= "table table-striped">
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>
  <th>City</th>
  <th></th>
  <th></th>
  <th></th>
  <th></th>

  <tbody>
    <% @cities.each do |city| %>
      
      <tr>  
        <td><%= city.name %> </td>
        <td><%= link_to 'Show', city %></td>
        <td><%= link_to 'Edit', edit_city_path(city) %></td>
        <td><%= link_to 'Destroy', city, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        <td><%= link_to "City Printers Index", city_printers_path(:city_id)%> </td>
        <td><%= link_to "Create New Printer List", new_city_printer_path(city_id: city.id) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
 
